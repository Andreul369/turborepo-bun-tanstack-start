    "@types/react-dom": "^19.2.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "camelcase-keys": "^10.0.1",
    "date-fns": "^4.1.0",
    "hono": "^4.12.3",
    "jose": "^6.1.3",
    "lucide-react": "^0.575.0",
