    "dev:website": "turbo dev --filter=@monorepo/website ",
    "dev:desktop": "turbo dev --filter=@monorepo/desktop",
    "jobs:dashboard": "turbo jobs --filter=@monorepo/jobs",
    "format": "biome format --write .",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck"
  },
  "packageManager": "bun@1.3.9",
  "catalog": {
